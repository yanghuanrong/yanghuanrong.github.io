default, 40%);\r\n\r\n    &.is-plain {\r\n      color: @color;\r\n    }\r\n\r\n    &:hover {\r\n      background-color: darken(@default, 5%);\r\n    }\r\n\r\n    &:active {\r\n      border-color: darken(@default, 10%);\r\n      background-color: fade(@default, 75%);\r\n    }\r\n\r\n    &:focus {\r\n      box-shadow: 0 0 0 2px fade(@default, 50%);\r\n    }\r\n\r\n    &.disabled,&.x-btn-loading {\r\n      &.is-plain {\r\n\r\n        &:hover,\r\n        &:focus,\r\n        &:active {\r\n          color: @color;\r\n          background-color: inherit !important;\r\n        }\r\n      }\r\n\r\n      &:hover,\r\n      &:focus,\r\n      &:active {\r\n        border-color: @default;\r\n        background-color: @color  !important;\r\n        box-shadow: none !important;\r\n      }\r\n    }\r\n  }\r\n\r\n  &.x-btn-info {\r\n    border-color: @info;\r\n    background-color: @info;\r\n\r\n    &.is-plain {\r\n      color: @info;\r\n    }\r\n\r\n    &:hover {\r\n      background-color: darken(@info, 5%);\r\n    }\r\n\r\n    &:active {\r\n      border-color: darken(@info, 10%);\r\n      background-color: fade(@info, 75%);\r\n    }\r\n\r\n    &:focus {\r\n      box-shadow: 0 0 0 2px fade(@info, 50%);\r\n    }\r\n\r\n    &.disabled,&.x-btn-loading {\r\n      &.is-plain {\r\n\r\n        &:hover,\r\n        &:focus,\r\n        &:active {\r\n          color: @info;\r\n          background-color: inherit !important;\r\n        }\r\n      }\r\n\r\n      &:hover,\r\n      &:focus,\r\n      &:active {\r\n        border-color: @info;\r\n        background-color: @info  !important;\r\n        box-shadow: none !important;\r\n      }\r\n    }\r\n  }\r\n\r\n  &.x-btn-success {\r\n    border-color: @success;\r\n    background-color: @success;\r\n\r\n    &.is-plain {\r\n      color: @success;\r\n    }\r\n\r\n    &:hover {\r\n      background-color: darken(@success, 5%);\r\n    }\r\n\r\n    &:active {\r\n      border-color: darken(@success, 10%);\r\n      background-color: fade(@success, 75%);\r\