        "name": "Button",
        "ext": "vue",
        "path": "../package/Button.vue",
        "core": "<script>\r\nexport default {\r\n  name: 'Button',\r\n  props: {\r\n    type: {\r\n      type: String,\r\n      default: 'default',\r\n      validator: value =>\r\n        [\r\n          'success',\r\n          'primary',\r\n          'warning',\r\n          'info',\r\n          'danger',\r\n          'default',\r\n          'text'\r\n        ].some(item => value === item)\r\n    },\r\n    icon: {\r\n      type: String,\r\n      default: ''\r\n    },\r\n    size: {\r\n      type: String,\r\n      default: 'md',\r\n      validator: value => ['lg', 'sm', 'md'].some(item => value === item)\r\n    },\r\n    plain: Boolean,\r\n    round: Boolean,\r\n    circle: Boolean,\r\n    block: Boolean,\r\n    disabled: Boolean,\r\n    loading: Boolean\r\n  },\r\n  render() {\r\n    \r\n    const className = {\r\n      class: [\r\n        this.type && 'x-btn-' + this.type,\r\n        this.size !== '' || this.size ? 'x-btn-' + this.size : '',\r\n        {\r\n          'is-plain': this.plain,\r\n          'is-round': this.round,\r\n          'is-circle': this.circle,\r\n          'is-block': this.block,\r\n          'disabled': this.disabled\r\n        },\r\n        this.loading && 'x-btn-loading'\r\n      ]\r\n    };\r\n\r\n    const slots = Object.keys(this.$slots).reduce(\r\n      (arr, key) => arr.concat(this.$slots[key]),\r\n      []\r\n    );\r\n\r\n    const listeners = {\r\n      on: !(this.loading || this.disabled) ? this.$listeners : null\r\n    };\r\n    return (\r\n      <button type='button' {...listeners}class='x-btn' {...className}>\r\n        {this.icon !== '' ? <i class={this.icon} /> : ''}\r\n        {this.loading && <i class='x-icon-loader' />}\r\n        {slots.length ? <span>{this.$slots.default}</span> : ''}\r\n      </button>\r\n    );\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n<style lang=\"less\" scope>\r\n  \r\n@default:#e3eaef;\r\n@success:#0acf97;\r\n@primary:#2d7bf4;\r\n@warning:#f9bc0b;\r\n@info:#4eb7eb;\r\n@danger:#f1556c;\r\n@white:#FFFFFF;\r\n@border:#d9e3e9;\r\n@text:#333;\r\n\r\n\r\n.x-btn-group {\r\n  position: relative;\r\n  display: -ms-inline-flexbox;\r\n  display: inline-flex;\r\n  vertical-align: middle;\r\n\r\n  &>.x-btn {\r\n    position: relative;\r\n    margin-left: -1px;\r\n    margin-right: 0 !important;\r\n    &:hover {\r\n      z-index: 1;\r\n    }\r\n  }\r\n\r\n  &>&:not(:last-child)>.x-btn,\r\n  &>.x-btn:not(:last-child):not(.dropdown-toggle) {\r\n    border-top-right-radius: 0;\r\n    border-bottom-right-radius: 0;\r\n  }\r\n\r\n  &>&:not(:first-child)>.x-btn,\r\n  &>.x-btn:not(:first-child) {\r\n    border-top-left-radius: 0;\r\n    border-bottom-left-radius: 0;\r\n  }\r\n}\r\n\r\n.x-btn {\r\n  display: inline-block;\r\n  position: relative;\r\n  max-width: 100%;\r\n  margin: 0;\r\n  padding: 7px 15px;\r\n  transition: all 0.25s ease-in-out;\r\n  border: 1px solid transparent;\r\n  border-radius: 2px;\r\n  color: #fff;\r\n  font-size: 12px;\r\n  font-weight: 400;\r\n  text-align: center;\r\n  text-overflow: ellipsis;\r\n  white-space: nowrap;\r\n  cursor: pointer;\r\n  vertical-align: middle;\r\n  -webkit-appearance: none;\r\n  -moz-appearance: none;\r\n  appearance: none;\r\n  -webkit-user-select: none;\r\n  -moz-user-select: none;\r\n  -ms-user-select: none;\r\n  user-select: none;\r\n  text-decoration: none;\r\n\r\n  &:focus {\r\n    background-image: none;\r\n    outline: 0;\r\n  }\r\n\r\n  &.x-btn-lg {\r\n    padding: 11px 23px;\r\n    font-size: 14px;\r\n  }\r\n\r\n  &.x-btn-sm {\r\n    padding: 3px 10px;\r\n    font-size: 12px;\r\n  }\r\n\r\n  &.x-btn-text {\r\n    background-color: transparent;\r\n    color: @primary;\r\n    padding-left: 0;\r\n    padding-right: 0;\r\n\r\n\r\n    &.disabled {\r\n      color: #999 !important;\r\n      background-color: inherit !important;\r\n\r\n      &:hover {\r\n        color: #999;\r\n      }\r\n    }\r\n  }\r\n\r\n  &.x-btn-default {\r\n    @color: #444;\r\n    color: @color;\r\n    border-color: @default;\r\n    background-color: fade(@default, 40%);\r\n\r\n    &.is-plain {\r\n      color: @color;\r\n    }\r\n\r\n    &:hover {\r\n      background-color: darken(@default, 5%);\r\n    }\r\n\r\n    &:active {\r\n      border-color: darken(@default, 10%);\r\n      background-color: fade(@default, 75%);\r\n    }\r\n\r\n    &:focus {\r\n      box-shadow: 0 0 0 2px fade(@default, 50%);\r\n    }\r\n\r\n    &.disabled,&.x-btn-loading {\r\n      &.is-plain {\r\n\r\n        &:hover,\r\n        &:focus,\r\n        &:active {\r\n          color: @color;\r\n          background-color: inherit !important;\r\n        }\r\n      }\r\n\r\n      &:hover,\r\n      &:focus,\r\n      &:active {\r\n        border-color: @default;\r\n        background-color: @color  !important;\r\n        box-shadow: none !important;\r\n      }\r\n    }\r\n  }\r\n\r\n  &.x-btn-info {\r\n    border-color: @info;\r\n    background-color: @info;\r\n\r\n    &.is-plain {\r\n      color: @info;\r\n    }\r\n\r\n    &:hover {\r\n      background-color: darken(@info, 5%);\r\n    }\r\n\r\n    &:active {\r\n      border-color: darken(@info, 10%);\r\n      background-color: fade(@info, 75%);\r\n    }\r\n\r\n    &:focus {\r\n      box-shadow: 0 0 0 2px fade(@info, 50%);\r\n    }\r\n\r\n    &.disabled,&.x-btn-loading {\r\n      &.is-plain {\r\n\r\n        &:hover,\r\n        &:focus,\r\n        &:active {\r\n          color: @info;\r\n          background-color: inherit !important;\r\n        }\r\n      }\r\n\r\n      &:hover,\r\n      &:focus,\r\n      &:active {\r\n        border-color: @info;\r\n        background-color: @info  !important;\r\n        box-shadow: none !important;\r\n      }\r\n    }\r\n  }\r\n\r\n  &.x-btn-success {\r\n    border-color: @success;\r\n    background-color: @success;\r\n\r\n    &.is-plain {\r\n      color: @success;\r\n    }\r\n\r\n    &:hover {\r\n      background-color: darken(@success, 5%);\r\n    }\r\n\r\n    &:active {\r\n      border-color: darken(@success, 10%);\r\n      background-color: fade(@success, 75%);\r\n    }\r\n\r\n    &:focus {\r\n      box-shadow: 0 0 0 2px fade(@success, 50%);\r\n    }\r\n\r\n    &.disabled,&.x-btn-loading {\r\n      &.is-plain {\r\n\r\n        &:hover,\r\n        &:focus,\r\n        &:active {\r\n          color: @success;\r\n          background-color: inherit !important;\r\n        }\r\n      }\r\n\r\n      &:hover,\r\n      &:focus,\r\n      &:active {\r\n        border-color: @success;\r\n        background-color: @success;\r\n        box-shadow: none !important;\r\n      }\r\n    }\r\n  }\r\n\r\n  &.x-btn-primary {\r\n    border-color: @primary;\r\n    background-color: @primary;\r\n\r\n    &.is-plain {\r\n      color: @primary;\r\n    }\r\n\r\n    &:hover {\r\n      background-color: darken(@primary, 5%);\r\n    }\r\n\r\n    &:active {\r\n      border-color: darken(@primary, 10%);\r\n      background-color: fade(@primary, 75%);\r\n    }\r\n\r\n    &:focus {\r\n      box-shadow: 0 0 0 2px fade(@primary, 50%);\r\n    }\r\n\r\n    &.disabled,&.x-btn-loading {\r\n      &.is-plain {\r\n\r\n        &:hover,\r\n        &:focus,\r\n        &:active {\r\n          color: @primary;\r\n          background-color: inherit !important;\r\n        }\r\n      }\r\n\r\n      &:hover,\r\n      &:focus,\r\n      &:active {\r\n        border-color: @primary;\r\n        background-color: @primary  !important;\r\n        box-shadow: none !important;\r\n      }\r\n    }\r\n  }\r\n\r\n  &.x-btn-warning {\r\n    border-color: @warning;\r\n    background-color: @warning;\r\n\r\n    &.is-plain {\r\n      color: @warning;\r\n    }\r\n\r\n    &:hover {\r\n      background-color: darken(@warning, 5%);\r\n    }\r\n\r\n    &:active {\r\n      border-color: darken(@warning, 10%);\r\n      background-color: fade(@warning, 75%);\r\n    }\r\n\r\n    &:focus {\r\n      box-shadow: 0 0 0 2px fade(@warning, 50%);\r\n    }\r\n\r\n    &.disabled,&.x-btn-loading {\r\n      &.is-plain {\r\n\r\n        &:hover,\r\n        &:focus,\r\n        &:active {\r\n          color: @warning;\r\n          background-color: inherit !important;\r\n        }\r\n      }\r\n\r\n      &:hover,\r\n      &:focus,\r\n      &:active {\r\n        border-color: @warning;\r\n        background-color: @warning  !important;\r\n        box-shadow: none !important;\r\n      }\r\n    }\r\n  }\r\n\r\n  &.x-btn-danger {\r\n    border-color: @danger;\r\n    background-color: @danger;\r\n\r\n    &.is-plain {\r\n      color: @danger;\r\n    }\r\n\r\n    &:hover {\r\n      background-color: darken(@danger, 5%);\r\n    }\r\n\r\n    &:active {\r\n      border-color: darken(@danger, 10%);\r\n      background-color: fade(@danger, 75%);\r\n    }\r\n\r\n    &:focus {\r\n      box-shadow: 0 0 0 2px fade(@danger, 50%);\r\n    }\r\n\r\n    &.disabled,&.x-btn-loading {\r\n      &.is-plain {\r\n\r\n        &:hover,\r\n        &:focus,\r\n        &:active {\r\n          color: @danger;\r\n          background-color: inherit !important;\r\n        }\r\n      }\r\n\r\n      &:hover,\r\n      &:focus,\r\n      &:active {\r\n        border-color: @danger;\r\n        background-color: @danger !important;\r\n        box-shadow: none !important;\r\n      }\r\n    }\r\n  }\r\n\r\n  &.is-plain {\r\n    border-width: 1px;\r\n    border-style: solid;\r\n    background-color: transparent;\r\n    box-shadow: none;\r\n\r\n    &:hover {\r\n      color: @white\r\n    }\r\n  }\r\n\r\n  &.x-btn-loading,\r\n  &.disabled,\r\n  &:disabled {\r\n    box-shadow: none;\r\n    opacity: 0.5;\r\n    cursor: not-allowed;\r\n\r\n    &:hover,\r\n    &:focus,\r\n    &:active {\r\n      transform: none;\r\n      box-shadow: none;\r\n      opacity: 0.5;\r\n    }\r\n  }\r\n\r\n\r\n  &.x-btn-loading {\r\n    cursor: auto;\r\n\r\n    &>i.x-icon-loader {\r\n      position: absolute;\r\n      animation: rotating linear 1.5s infinite;\r\n\r\n      &+span {\r\n        margin-left: 15px;\r\n        transition: margin-left .2s linear;\r\n      }\r\n    }\r\n  }\r\n\r\n\r\n\r\n  &.is-round {\r\n    border-radius: 2em\r\n  }\r\n\r\n  &.is-circle {\r\n    border-radius: 50%;\r\n    padding: 8px 9px;\r\n  }\r\n\r\n  &.is-block {\r\n    display: block;\r\n    width: 100%;\r\n  }\r\n\r\n  & [class*=x-icon-]+span {\r\n    margin-left: 5px;\r\n  }\r\n}\r\n\r\n@keyframes rotating {\r\n  0% {\r\n    transform: rotate(0);\r\n  }\r\n\r\n  100% {\r\n    transform: rotate(360deg);\r\n  }\r\n}\r\n</style>"
